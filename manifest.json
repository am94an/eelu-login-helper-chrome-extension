{
    "manifest_version": 3,
    "name": "EEU Login Helper",
    "version": "1.0",
    "permissions": [
      "cookies",
      "storage",
      "scripting",
      "declarativeNetRequest"
    ],
    "host_permissions": [
      "https://*.eelu.edu.eg/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }